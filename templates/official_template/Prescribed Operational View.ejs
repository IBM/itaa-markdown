    <% } %>
    <% if (ele.os) { %>
    <tr>
        <td> <strong>OS/Hyper-visor</strong> </td>
        <td><%- ele.os %></td>
    </tr>
    <% } %>
    <% if (ele.offering) { %>
    <tr>
        <td> <strong>Deploy</strong> </td>
        <td><%- ele.offering %></td>
    </tr>
    <% } %>
    <% if (ele.provision) { %>
    <tr>
        <td> <strong>Deploy Provisioning</strong> </td>
        <td><%- ele.provision %></td>
    </tr>
    <% } %>
</table>


</details>


    <% }); %>
<% } %>

<% } %>
